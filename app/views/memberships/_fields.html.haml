%table.tall.shaded
  %tr
    %th
      From
    %td
      = date_select "membership[from]", @membership.from, :id => "from"
  - if @membership.model.allow_overlap
    %tr
      %th
        To
      %td
        = date_select "membership[upto]", (@membership.upto == SEP_DATE ? nil : @membership.to), :id => "upto"
  